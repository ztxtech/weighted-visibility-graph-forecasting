    "from pylab import *\n",
    "k=0\n",
    "x=linspace(0,2*pi,100)\n",
    "y=sin(x)\n",
    "y1=cos(x)\n",
    "for i in style.available:\n",
    "    style.use(i)\n",
    "    title(i)\n",
    "    plot(x,y,'-o')\n",
    "    plot(x,y1,'-+')\n",
    "    k+=1\n",
    "    show()"
